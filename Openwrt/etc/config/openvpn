
config openvpn 'pia_client'
        option dev 'tun'
        option comp_lzo 'yes'
        option nobind '1'
        option keepalive '10 120'
        option persist_tun '1'
        option persist_key '1'
        option client '1'
        option auth_user_pass '/etc/openvpn/userpass.txt'
        list remote 'swiss.privateinternetaccess.com'
        list remote 'us-east.privateinternetaccess.com'
        option proto 'udp'
        option resolv_retry 'infinite'
        option tls_client '1'
        option ca '/etc/luci-uploads/cbid.openvpn.pia_client.ca'
        option reneg_sec '0'
        option auth_nocache '1'
        option remote_cert_tls 'server'
        option crl_verify '/etc/openvpn/crl.rsa.2048.pem'
        option port '1198'
        option auth 'SHA1'
        option cipher 'AES-128-CBC'
        option enabled '1'
        option verb '3'
